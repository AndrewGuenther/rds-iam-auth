{
  "name": "rds-iam-auth",
  "version": "1.0.0",
  "description": "RDS IAM Authentication Example",
  "scripts": {
    "clean": "rimraf cdk.out coverage",
    "deploy": "npm run clean && cdk deploy --require-approval never --outputs-file config.json",
    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "synth": "cdk synth --no-staging > template.yaml",
    "test": "jest --coverage --silent"
  },
  "keywords": [
    "AWS",
    "CDK",
    "IAM",
    "RDS",
    "Authentication",
    "MySQL",
    "PostgreSQL"
  ],
  "author": "Matt Morgan",
  "license": "ISC",
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.133.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.133.0",
    "@aws-cdk/aws-ec2": "1.133.0",
    "@aws-cdk/aws-iam": "1.133.0",
    "@aws-cdk/aws-lambda": "1.133.0",
    "@aws-cdk/aws-lambda-nodejs": "1.133.0",
    "@aws-cdk/aws-logs": "1.133.0",
    "@aws-cdk/aws-rds": "1.133.0",
    "@aws-cdk/core": "1.133.0",
    "@aws-cdk/custom-resources": "1.133.0",
    "aws-sdk": "^2.1033.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.133.0",
    "@types/aws-lambda": "^8.10.73",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "aws-cdk": "1.133.0",
    "esbuild": "^0.11.5",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.2"
  }
}
